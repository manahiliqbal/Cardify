{
    "builds": [
      {
        "src": "flashcards-server/server.js",    
        "use": "@vercel/node"     
      },
      {
        "src": "flashcards-client/package.json",   
        "use": "@vercel/static-build"   
      }
    ],
    "routes": [
      {
        "src": "/flashcards-server/(.*)",        
        "dest": "/flashcards-server/index.js"    
      },
      {
        "src": "/(.*)",            
        "dest": "/flashcards-client/$1"       
      }
    ]
  }
  